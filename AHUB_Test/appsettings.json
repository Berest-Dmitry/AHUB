{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "JWTAuthenticationServer",
    "Audience": "JWTServicePostmanClient",
    "Subject": "JWTServiceAccessToken",
    "RefreshTokenExpirationTime": "6"
  },
  "RedisConnection": {
    "localAddress": "127.0.0.1:6379",
    "dockerAddress": "cache:6379"
  },
  "SmsServer": {
    "Url": "https://sms.ru/sms",
    "TestMode": 1
  },
  "HashSettings": {
    "keySize": 64,
    "iterations": 350000
  },
  "MinioSettings": {
    "accessKey": "kKhpPSoEV1CgMSZRH17S",
    "endpoint": "127.0.0.1:9000",
    "bucketName": "test"
  },
  "RateLimiterSettings": [
    {
      "expiry": 10,
      "maxRequests": 25,
      "Path": ""
    },
    {
      "expiry": 10,
      "maxRequests": 50,
      "PathRegex": "/api/"
    },
    {
      "expiry": 10,
      "maxRequests": 10,
      "PathRegex": "/swagger"
    }
  ],
  "ELKConfiguration": {
    "UrlLocal": "https://localhost:9200",
    "UrlDokcer": "https://host.docker.internal:9200",
    "index": "posts",
    "Fuzzy": {
      "max_expansions": 100,
      "prefix_length": 0,
      "boost": 1.1
    }
  },
  "Rabbit": {
    "host": "localhost",
    "queue": "MyQueue",
    "clusteringQueue": "CTQ",
    "chunkSizeMb":  1
  }
}
